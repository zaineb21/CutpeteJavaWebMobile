{% extends 'base/baseadmin.html.twig' %}

{% block titre %}  Veterinaire  {% endblock %}

{% block body %}
    <h1>Veterinaire</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Picture</th>
            <td>{{ veterinaire.picture }}</td>
        </tr>
            <tr>
                <th>Nom</th>
                <td>{{ veterinaire.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ veterinaire.prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ veterinaire.email }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ veterinaire.address }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ veterinaire.date |date("m/d/Y")}}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ veterinaire.numero }}</td>
            </tr>
        </tbody>
    </table>
    <button class="btn btn-danger" ><a href="{{ path('app_veterinaire_index')}}" style="color:white">Retourner</a></button>


{% endblock %}
