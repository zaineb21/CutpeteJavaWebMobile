
{% extends 'baseFront.html.twig ' %}

{% block titre %}
    Reclamation
{% endblock %}

{% block section%}
{% endblock %}
{% block corp%}
    <h1>Commande index</h1>

    <table class="table">
        <thead>
        <tr>
            <th>IdCommande</th>
            <th>AdresseCommande</th>
<th>etat</th>
            <th>actions</th>
        </tr>
        </thead>
        <tbody>
        {% for commande in commandes %}
            <tr>
                <td>{{ commande.idCommande }}</td>
                <td>{{ commande.adresseCommande }}</td>
<td>{{ commande.etatCommande }}</td>
                <td>
                    <a href="{{ path('app_commande_show', {'idCommande': commande.idCommande}) }}">show</a>
                    <a href="{{ path('app_commande_edit', {'idCommande': commande.idCommande}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_commande_new') }}">Create new</a>
{%endblock%}



