
{% extends 'baseFront.html.twig ' %}

{% block titre %}
    Mes produits
{% endblock %}

{% block section%}
{% endblock %}

{% block corp%}
    <h1>Produit</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>IdProduit</th>
            <td>{{ produit.idProduit }}</td>
        </tr>
        <tr>
            <th>LibelleProduit</th>
            <td>{{ produit.libelleProduit }}</td>
        </tr>
        <tr>
            <th>Categorie</th>
            <td>{{ produit.categorie }}</td>
        </tr>
        <tr>
            <th>Prix</th>
            <td>{{ produit.prix }}</td>
        </tr>
        <tr>
            <th>DateExpiration</th>
            <td>{{ produit.dateExpiration ? produit.dateExpiration|date('Y-m-d') : '' }}</td>
        </tr>
        <tr>
            <th>Quantite</th>
            <td>{{ produit.quantite }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ produit.description }}</td>
        </tr>
        <tr>
            <th>Note</th>
            <td>{{ produit.note }}</td>
        </tr>
        <tr>
            <th>Image</th>
            <td>{{ produit.image }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('app_p_indexF') }}">back to list</a>





{% endblock %}

